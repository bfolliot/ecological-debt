    "type": "project",
    "authors": [
        {
            "name": "Bryan Folliot",
            "email": "dev@bryanfolliot.fr"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "EcologicalDebt\\": "src/"
        }
    },
